<?xml version="1.0"?>
  <!-- engine rpm instrument -->
  <PropertyList> 
    
	<path>rpm.ac</path>
    
	<animation> 
      <type>rotate</type>
      <object-name>rpm-Needle1</object-name>
      <property>engines/engine/rpm</property>
      <interpolation> 
        <entry>
          <ind>0</ind>
          <dep>245</dep>
        </entry>
        <entry>
          <ind>1000</ind>
          <dep>240</dep>
        </entry>
        <entry>
          <ind>1500</ind>
          <dep>210</dep>
        </entry>
        <entry>
          <ind>2000</ind>
          <dep>180</dep>
        </entry>
        <entry>
          <ind>2500</ind>
          <dep>135</dep>
        </entry>
        <entry>
          <ind>3000</ind>
          <dep>90</dep>
        </entry>
        <entry>
          <ind>3500</ind>
          <dep>45</dep>
        </entry>
        <entry>
          <ind>4000</ind>
          <dep>0</dep>
        </entry>
        <entry>
          <ind>4500</ind>
          <dep>-30</dep>
        </entry>
        <entry>
          <ind>5000</ind>
          <dep>-60</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>
    
  </PropertyList>
