<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>magneto.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>Clutch.guard</object-name>
    <object-name>Clutch.switch</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>Clutch.guard</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine/magnetoguardL</property>
      </binding>
      <!--<binding>
        <command>property-toggle</command>
        <property>sim/model/sound/click</property>
      </binding>-->
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>Clutch.switch</object-name>
    <action>
      <button>0</button>
      <binding>
        <condition>
          <equals>
            <property>controls/engines/engine/magnetoguardL</property>
            <value>true</value>
          </equals>
        </condition>
        <command>property-toggle</command>
        <property>controls/engines/engine/magnetoL</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/sound/click</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Clutch.guard</object-name>
    <property>controls/engines/engine/magnetoguardL</property>
    <factor> -80 </factor>
    <center>
      <x-m>  0.001 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.013 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Clutch.switch</object-name>
    <property>controls/engines/engine/magnetoL</property>
    <factor> 30 </factor>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

