<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>switch.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>switch</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>switch</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine[0]/starter</property>
      </binding>
      <mod-up>
        <binding>
          <command>property-toggle</command>
          <property>controls/engines/engine[0]/starter</property>
          <value>0</value>
        </binding>
      </mod-up>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/sound/click</property>
        <value>1</value>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>switch</object-name>
    <property>controls/engines/engine[0]/starter</property>
    <factor> 60 </factor>
    <axis>
      <x>  0 </x>
      <y>  1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
